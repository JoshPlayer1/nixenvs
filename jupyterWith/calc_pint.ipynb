   "source": [
    "from pint import UnitRegistry\n",
    "import math\n",
    "u = UnitRegistry()\n",
    "R = 130/2 *u.cm\n",
    "A = math.pi*R**2\n",
    "h = 1 *u.cm\n",
    "V = A*h\n",
    "rho = 1.25 *(u.g/u.cm**3)\n",
    "m = V * rho\n",
    "print(A.to(u.m**2))\n",
    "print(V.to(u.l))\n",
    "print(m.to(u.kg))"
   ]
